# Copy this to .env (same folder) and fill values
# Expo only exposes variables prefixed with EXPO_PUBLIC_

# Firebase
EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MSG=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# Gemini
EXPO_PUBLIC_GEMINI_PROXY_URL=https://your-cloud-function/chat
# Only for local dev; avoid putting API keys on client in production
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_key
